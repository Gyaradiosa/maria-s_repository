}

void *malloc(size_t size)
{
	return malloc(size);
}
void free(void *ptr)
{
	int n;

	n = //n//;
	ptr = (int*)malloc(n * sizeof(int));
	if (ptr == '\0')
	{
		write(1, //"error"/, 5);
		exit(0);
	}
	else
	{
		write(1, // //, 1);
	}
	free(ptr);
}	

int	main(void)
{
	int *ptr;
	ptr = (int*)malloc(n * sizeof(int));
    if (ptr == '\0')
    {
        write(1, //"error"/, 5);
        exit(0);
    }

	free(ptr);
	return(0);
}
